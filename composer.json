{
  "name": "nikitanp/laravel-discourse",
  "description": "PHP Discourse Forum API for the Laravel Framework",
  "type": "library",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Matthew Jensen",
      "email": "matt@happyhousemedia.com"
    },
    {
      "name": "Nikita Mikhno",
      "email": "nikita.mikhnonp@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^4",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "NikitaMikhno\\LaravelDiscourse\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "NikitaMikhno\\LaravelDiscourse\\DiscourseServiceProvider"
      ],
      "aliases": {
        "Discourse": "NikitaMikhno\\LaravelDiscourse\\Facades\\Discourse",
        "DiscourseAuth": "NikitaMikhno\\LaravelDiscourse\\Facades\\DiscourseAuth"
      }
    }
  }
}
